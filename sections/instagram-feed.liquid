<style>
  .logo-insta {width: 100%;display: block;text-align: center;text-transform: uppercase;margin-bottom: 15px;}
  .logo-insta img {width: 25px;vertical-align: middle;margin: 0 5px;}
  #instafeed {margin: 0;}
  #instafeed li {float: left;width: 20%;height: 228px;margin: 0;list-style-type: none;}
  #instafeed li a, #instafeed li img {display: block;width: 100%;}
  @media (max-width: 991px){
    #instafeed li {width: 25%;}
  }
  @media (max-width: 480px){
    #instafeed li {width: 33.333%;}
  }
</style>
<div id="section-insta">
  <div class="wrapper">
    <div id="insta-feed"></div>
  </div>
</div>

