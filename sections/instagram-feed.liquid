{{ 'instafeed.min.js' | asset_url | script_tag }} 
<script>
  var feed = new Instafeed({
    get: 'user',
    userId: 202599408,
    links: 'true',
    limit: 10,
    resolution: 'low_resolution',
    accessToken: '202599408.1677ed0.a2ffe76a59134d4ebec9de47bed4deb3',
    {% raw %}template: "<li class='image-instagram swiper-slide'><a href='{{link}}'><img src='{{image}}' /></a></li>"{% endraw %}
  });
  feed.run();
  var swiper = new Swiper('.swiper-instafeed', {
    slidesPerView: 5,
    loop: true,
    navigation: {
      nextEl: '.carousel-button-next',
      prevEl: '.carousel-button-prev'
    },
  });
</script>
